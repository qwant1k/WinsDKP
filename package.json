{
  "name": "ymir-clan-hub",
  "version": "1.0.0",
  "private": true,
  "description": "Ymir Clan Hub â€” MMORPG Clan Management System",
  "packageManager": "pnpm@9.15.4",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "test:e2e": "turbo run test:e2e",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rimraf node_modules",
    "db:migrate": "pnpm --filter @ymir/api run db:migrate",
    "db:seed": "pnpm --filter @ymir/api run db:seed",
    "db:generate": "pnpm --filter @ymir/api run db:generate",
    "db:studio": "pnpm --filter @ymir/api run db:studio"
  },
  "devDependencies": {
    "turbo": "^2.3.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3"
  }
}
